Treat DragonFly like FreeBSD.

--- mozilla/js/src/Makefile.in.orig	2009-06-29 18:14:51.000000000 +0200
+++ mozilla/js/src/Makefile.in
@@ -429,6 +429,9 @@ endif # WINNT
 ifeq ($(OS_ARCH),FreeBSD)
 EXTRA_LIBS	+= -pthread
 endif
+ifeq ($(OS_ARCH),DragonFly)
+EXTRA_LIBS	+= -pthread
+endif
 ifeq ($(OS_ARCH),IRIX)
 ifdef USE_N32
 DASH_R		+= -n32
